}

.operations-buttons {
  display: flex;
  justify-content: space-around;
  gap: 10px;
}

.operations-buttons button {
  background-color: #186ff2;
  color: rgb(143, 21, 21);
  border: none;
  padding: 10px 18px; /* Slightly smaller padding */
  font-size: 14px;
  font-weight: bold;
  border-radius: 8px;
  cursor: pointer;
  transition: transform 0.2s ease, background-color 0.3s ease;
}

.operations-buttons button:hover {
  background-color: #135bb5;
  transform: scale(1.05);
}


/* Footer Styling */
footer {
  background-color: rgba(24, 111, 242, 0.8);
  color: #ffffff;
  text-align: center;
  padding: 10px;
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  font-size: 14px;
}



html, body {
  height: 100%; /* Ensure full height layout */
}

body {
  display: flex;
  flex-direction: column; /* Stack header, content, and footer vertically */
}

.container {
  flex: 1; /* Allow the container to take up the remaining space */
}
/* General Reset */
body, html {
    margin: 0;
    padding: 0;
    box-sizing: border-box;
    font-family: Arial, sans-serif;
    background-color: #1e293b; /* Dark blue background for modern look */
    color: #ffffff;
}

/* Center Container */
.container {
    max-width: 800px;
    margin: 50px auto;
    padding: 20px;
    background-color: #2d3748; /* Slightly lighter for content box */
    border-radius: 10px;
    box-shadow: 0 4px 8px rgba(0, 0, 0, 0.2);
    text-align: center;
}

/* Title */
h2 {
    margin-bottom: 20px;
    font-size: 24px;
    color: #63b3ed; /* Light blue title */
}

/* Form Styling */
form {
    margin-bottom: 20px;
}

label {
    font-size: 16px;
    margin-right: 10px;
    color: #a0aec0;
}

input, select, button {
    width: calc(100% - 20px);
    padding: 10px;
    margin-bottom: 15px;
    font-size: 14px;
    border: 1px solid #4a5568; /* Light gray for borders */
    border-radius: 5px;
    background-color: #2c3e50; /* Dark input background */
    color: #ffffff;
    outline: none;
}

input::placeholder {
    color: #a0aec0;
}

button {
    width: auto;
    padding: 10px 20px;
    font-size: 16px;
    background-color: #3182ce; /* Blue button */
    border: none;
    border-radius: 5px;
    cursor: pointer;
    color: #ffffff;
    transition: background-color 0.3s;
}

button:hover {
    background-color: #2b6cb0; /* Darker blue on hover */
}

/* Results Section */
#results {
    display: none; /* Hidden initially */
}

h3 {
    color: #edf2f7;
    margin-bottom: 10px;
}

/* Table Styling */
table {
    width: 100%;
    border-collapse: collapse;
    margin-top: 20px;
}

thead {
    background-color: #4a5568; /* Gray background for table headers */
    color: #ffffff;
}

th, td {
    padding: 10px;
    text-align: left;
    border: 1px solid #2c3e50; /* Border around table cells */
}

tbody tr:nth-child(even) {
    background-color: #2d3748; /* Alternating row color */
}

tbody tr:nth-child(odd) {
    background-color: #3b4956;
}

td {
    color: #edf2f7;
}

/* Responsive Design */
@media (max-width: 600px) {
    input, select, button {
        width: 100%;
    }

    table {
        font-size: 12px;
    }
}


